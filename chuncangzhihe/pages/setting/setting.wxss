.main {
    padding: 30rpx;
    background-color: rgba(30, 30, 30, 1);
}

.part {
    margin-bottom: 30rpx;
    border: 1rpx #d4d4d4 solid;
    border-radius: 10rpx;
    padding: 30rpx;
    background-color: rgba(255, 255, 255, .9);
    /* -webkit-box-shadow: 8rpx 8rpx 10rpx #999; */
}

.addNew {
    position: relative;
    height: 100rpx;
    width: 100rpx;
    margin: 0 auto;
}

.addNew::before {
    content: '';
    width: 100rpx;
    height: 10rpx;
    background-color: #e2e2e2;
    position: absolute;
    top: 45rpx;
    left: 0;
    border-radius: 10rpx;
}

.addNew::after {
    content: '';
    width: 10rpx;
    height: 100rpx;
    background-color: #e2e2e2;
    position: absolute;
    top: 0;
    left: 45rpx;
    border-radius: 10rpx;
}

.list {
    display: -webkit-box;
    height: 80rpx;
    line-height: 80rpx;
    margin-bottom: 30rpx;
}

.list:last-child {
    margin-bottom: 0;
}

.title {
    width: 120rpx;
    text-align-last: justify;
    -webkit-text-align-last: justify;
    font-size: 28rpx;
    color: #666;
    margin-right: 50rpx;
}

.inPut {
    border: 1rpx #999 solid;
    border-radius: 10rpx;
    -webkit-box-flex: 1;
    padding: 0 20rpx;
    height: 80rpx;
}

.picker {
    height: 80rpx;
    -webkit-box-flex: 1;
    font-size: 32rpx;
    color: #666;
    margin-right: 40rpx;
}

.singleIn {
    border: 1rpx #999 solid;
    border-radius: 10rpx;
    height: 80rpx;
    width: 80rpx;
    margin-right: 10rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 48rpx;
    color: #000;
}

.singleIn:last-child {
    margin-right: 0;
}

.fIn {
    border: 1rpx #999 solid;
    border-radius: 10rpx;
    height: 80rpx;
    width: 180rpx;
    margin-right: 10rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #000;
    padding: 0 10rpx;
}